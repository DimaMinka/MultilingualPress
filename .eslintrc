{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "_": false,
    "Backbone": false
  },
  "extends": "eslint:recommended",
  "rules": {
    "consistent-return": 2,
    "curly": [ 2, "all" ],
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-caller": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 0,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-return-assign": 2,
    "no-undef": 2,
    "no-use-before-define": [ 2, "nofunc" ],
    "no-useless-call": 2,
    "no-with": 2,
    "radix": 2,
    "strict": 0,
    "vars-on-top": 2,
    "wrap-iife": [ 2, "inside" ],
    "yoda": [ 2, "always" ]
  }
}
